    ['orangeFont', 'orangeFontHover', 'orangeBg', 'orangeBgHover'],
    ['silverFont', 'silverFontHover', 'silverBg', 'silverBgHover'],
    ['redFont', 'redFontHover', 'redBg', 'redBgHover'],
    ['emeraldFont', 'emeraldFontHover', 'emeraldBg', 'emeraldBgHover']];
  var mood = (Math.floor(Math.random() * colors.length + 1)) - 1;
  var fontColors0 = ['.project-container strong', '.project-container > h1', '.project-container a', '.site-title > span.droid', 'ul.main-menu > li > a'];
  var fontColors1 = ['.project-container a:hover', 'ul.main-menu > li > a:hover', 'a:hover', '.site-title > span.droid:hover'];
  var bgColors0 = ['ul.projects > li', '.logo'];
  var bgColors1 = ['ul.projects > li .project-shadow a', '.logo:hover'];
  fontColors0.forEach(function(i){
    $(i).addClass(colors[mood][0]);
  })
  fontColors1.forEach(function(i){
    $(i).addClass(colors[mood][1]);
  })
  bgColors0.forEach(function(i){
    $(i).addClass(colors[mood][2]);
  })
  bgColors1.forEach(function(i){
    $(i).addClass(colors[mood][3]);
  })*/
});;T;	@#;
